<h1> <%= t('produtos.index.titulo') %> </h1>
<table class="zebra-striped">
	<thead>
		<tr>
			<th>Nome</th>
			<th>Preco</th>
		</tr>
	</thead>
	<tbody>
		<% @produtos.each do |produto|%>
		<tr>
			<td><%=link_to produto.nome , produto_url(produto) %></td>
			<td><%= number_to_currency produto.preco %></td>
		</tr>
		<%end%>
	</tbody>
</table>
